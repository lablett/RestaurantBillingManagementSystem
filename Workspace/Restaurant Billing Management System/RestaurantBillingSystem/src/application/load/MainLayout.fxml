<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="bpMain" prefHeight="520.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.load.MainLayoutController">
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
          <Button fx:id="btnnHome" mnemonicParsing="false" onAction="#goHome" text="Home" />
        </items>
      </ToolBar>
   </top>
   <bottom>
      <ToolBar prefHeight="40.0" prefWidth="277.0" BorderPane.alignment="CENTER">
        <items>
            <Button fx:id="btnnLogOut" mnemonicParsing="false" onAction="#goLogOut" text="Log Out" />
            <Button fx:id="btnnExit" mnemonicParsing="false" onAction="#exitProgram" text="Exit" />
        </items>
      </ToolBar>
   </bottom>
</BorderPane>
